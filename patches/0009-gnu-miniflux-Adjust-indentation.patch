+      #:go go-1.22
+      #:install-source? #f
+      #:import-path "miniflux.app/v2"
+      #:build-flags
+      #~(list (string-append
+               "-ldflags= -X miniflux.app/v2/internal/version.Version="
+               #$version))
+      #:phases
+      #~(modify-phases %standard-phases
