+             (commit version)))
+       (file-name (git-file-name name version))
+       (sha256
+        (base32 "0cb0fkjzbjrhgmx1s8jv1drdgfaddkyj9j9z451qswgvz6xyp3a3"))))
     (build-system go-build-system)
     (arguments
